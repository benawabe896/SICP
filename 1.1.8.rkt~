"Start 1.8"
(define (cubed x)(* x x x))
(define (improve guess x)
   (average guess (/ x guess)))
(define (good-enough? guess x)
  (< (abs (- (cubed guess) x)) 0.001))
(define (curt-iter guess x)
  (if (good-enough? guess x)
      guess
      (curt-iter (improve guess x)
                 x)))
(define (curt x)
  (curt-iter 1.0 x))

(curt 8)